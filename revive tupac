local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "revive tupac", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
OrionLib:MakeNotification({
	Name = "Just use a library",
	Content = "justin just did this in like 10 minutes lol",
	Image = "rbxassetid://4483345998",
	Time = 5
})
local CBRO = Window:MakeTab({
	Name = "CBRO",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
CBRO:AddButton({
	Name = "hexagon",
	Callback = function()
	    if game.PlaceId == 301549746
	    then
	    loadstring(game:HttpGet("https://raw.githubusercontent.com/emptiness6969/rblxscripts/main/hexagonfix.lua"))()
	    OrionLib:MakeNotification({
	Name = "Success!",
	Content = "Enjoy being gay!",
	Image = "rbxassetid://4483345998",
	Time = 5
})
else
    wait()
    OrionLib:MakeNotification({
	Name = "Oh no!",
	Content = "It appears this script is not compatible with this game.",
	Image = "rbxassetid://4483345998",
	Time = 5
})
	    end
	    end
})
CBRO:AddButton({
	Name = "Multi-script loader",
	Callback = function()
	    if game.PlaceId == 301549746
	    then
	    loadstring(game:HttpGet("https://raw.githubusercontent.com/Danixik/LoaderV3/main/Main.lua", true))()
	    OrionLib:MakeNotification({
	Name = "Success!",
	Content = "Enjoy being gay!",
	Image = "rbxassetid://4483345998",
	Time = 5
})
else
    wait()
    OrionLib:MakeNotification({
	Name = "Oh no!",
	Content = "It appears this script is not compatible with this game.",
	Image = "rbxassetid://4483345998",
	Time = 5
})
end
end
})
